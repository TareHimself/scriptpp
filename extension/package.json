{
  "name": "spp",
  "displayName": "ScriptPP",
  "description": "Vs Code Support for Script PP Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "spp",
      "aliases": ["ScriptPP", "spp"],
      "extensions": [".spp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "spp",
      "scopeName": "source.spp",
      "path": "./syntaxes/scriptpp.tmLanguage.json"
    }]
  }
}
